{
  "name": "lidercap/framework-component-listener",
  "description": "Listener de eventos",
  "version": "1.0.0",
  "license": "proprietary",
  "type": "library",
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Lidercap\\Component\\Listener\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lidercap\\Tests\\Component\\Listener\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "lidercap/framework-core": "^1.1"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "phpunit/phpunit": "^4.8",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "2.0.*",
    "sebastian/phpdcd": "^1.0",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:lidercap/framework-core.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
